<h1>Log In</h1>

<% if current_user %>
	<%=current_user.name %>
<% end %>

<%=form_tag log_in_path do%>
<%=label_tag :email %>
<br>
<%=text_field_tag :email %>
<br>
<%=label_tag :password %>
<br>
<%=password_field_tag :password %>
<br>
<%=submit_tag "Log in" %>
<%end%>
<br>
<%= link_to "Create Account", new_user_path %>